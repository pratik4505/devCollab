{
  "name": "devcollab",
  "displayName": "DevCollab",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "webview",
          "title": "Example for webview",
          "icon": "./assets/logo.png"
        }
      ]
    },
    "views": {
      "webview": [
        {
          "type": "webview",
          "id": "left-panel-webview",
          "name": "Webview",
          "icon": "./assets/logo.png"
        }
      ]
    },
    "commands": [
      {
        "command": "devcollab.start",
        "title": "start devcollab"
      },
      {
        "command": "devcollab.chat",
        "title": "open chat panel"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "vscode": "^1.1.37"
  },
  "icon": "./assets/logo.png",
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.5",
    "vsls": "^1.0.4753"
  }
}
